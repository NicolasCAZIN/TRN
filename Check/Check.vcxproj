SETLOCAL EnableExtensions
SETLOCAL EnableDelayedExpansion

FOR %%h IN (BOOST, OPENCV,  CUDA, MKL, MPI, STRTK, MATLAB, MEXPLUS, ZEROMQ) DO (
CALL :check_path %%h_PATH
)

FOR %%h IN (TRN_SIMULATOR, TRN_EXPERIMENT) DO (
CALL :check_path %%h
)

GOTO:install_ok

:check_path 
echo Checking %1 path configuration
set key=%1
set value=!%key%!

IF NOT DEFINED %key% (
 ECHO Variable %key% is not defined
 EXIT 1
)

IF "%value%" == "" (
 ECHO Variable %key% is empty
 EXIT 1
)

IF NOT EXIST "%value%"  (
 ECHO Directory "%value%" does not exist
 EXIT  1
)

ECHO %1 is pointing to %value%
GOTO :eof

:install_ok
ECHO All required paths have been found
EXIT 0</Command>
      <Message>Checking required libraries installation</Message>
